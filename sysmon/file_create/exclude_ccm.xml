<Sysmon schemaversion="4.22">
   <EventFiltering>
	  <RuleGroup groupRelation="or">
         <FileCreate onmatch="exclude">
            <Image condition="is">C:\WINDOWS\CCM\CcmExec.exe</Image>
			<TargetFilename condition="begin with">C:\Windows\CCM</TargetFilename>
         </FileCreate>
      </RuleGroup>
   </EventFiltering>
</Sysmon>
